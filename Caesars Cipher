function rot13(str) {

  let strArr = str.split('');

  for (let i=0; i < strArr.length; i++) {
    if ((strArr[i].charCodeAt(0) - "A".charCodeAt(0) >= 0) && (strArr[i].charCodeAt(0) - "A".charCodeAt(0) < 26)) {
      strArr[i] = String.fromCharCode( ( (strArr[i].charCodeAt(0) - "A".charCodeAt(0) + 13) % 26 ) + "A".charCodeAt(0))
    }
  }
  let newStr = strArr.join(''); 
  //console.log(newStr)
  return newStr;
}

rot13("SERR PBQR PNZC");
